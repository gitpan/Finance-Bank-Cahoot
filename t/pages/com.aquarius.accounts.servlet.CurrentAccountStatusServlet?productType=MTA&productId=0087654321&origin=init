function openBrWindow(theURL,winName,x,y) { //v2.0
  remote = window.open(theURL, winName, 'height=' + x + ',width=' + y + ',scrollbars=no,menubar=yes,toolbar=no');
	  if (remote != null) {
	  remote.focus();
	  } 
}
function makeRemote(url) {
	remote = window.open(url, 'help', 'height=329,width=509,scrollbars=yes');
	if (remote != null) {
	  	remote.focus();
	  	}
    if (remote.opener == null) remote.opener = window;
	remote.opener.name = "opener"; 
}


	 
	
	  function clickHere() {      
	  openBrWindow('help/frameset_help_ucl_balance_trans.html', 'clickHere', '329','509');
	  }
	  
